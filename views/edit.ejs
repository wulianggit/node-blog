<%- include header%>
<form action="/edit/<%= post.name%>/<%= post.time.day%>/<%= post.title%>" method="post">
	标题:<br>
	<input type="text" name="title" disabled value="<%= post.title%>"><br/>
	正文：<br>
	<textarea name="post" cols="100" rows="20"><%= post.post%></textarea><br>
	<input type="submit" value="保存修改">
</form>
<%- include footer%>